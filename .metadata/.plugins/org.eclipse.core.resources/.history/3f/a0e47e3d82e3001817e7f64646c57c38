package LogWorld;

import java.util.Calendar;

public class test {

	public static void main(String[] args) {
		System.out.println(getTime());
	}
	private static String getTime() {
    	return Calendar.getInstance().get(Calendar.YEAR)+"/" + (Calendar.getInstance().get(Calendar.MONTH) + 1) + "/" + Calendar.getInstance().get(Calendar.DAY_OF_MONTH)+" "+Calendar.getInstance().get(Calendar.HOUR_OF_DAY)+":"+Calendar.getInstance().get(Calendar.MINUTE)+":"+Calendar.getInstance().get(Calendar.SECOND);
    }
}
